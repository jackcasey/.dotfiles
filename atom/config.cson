"*":
  "activate-power-mode":
    autoToggle: false
  "atom-alignment": {}
  "atom-beautify":
    general:
      _analyticsUserId: "c7c30fc1-1969-4b83-9f34-eeb8fbad1ca6"
      analytics: false
      beautifyEntireFileOnSave: false
    js:
      indent_size: 2
  "atom-ctags":
    autoBuildTagsWhenActive: true
    buildTimeout: 15000
    cmdArgs: "--exclude=.git --exclude=log --exclude=coverage --exclude=tmp --exclude=vendor --exclude=node_modules --exclude=spec --exclude=fixtures --exclude=features --exclude=db --exclude=bin --exclude=public --exclude=tmp --exclude=app/assets"
  "atom-fuzzy-grep": {}
  atomatigit: {}
  "autocomplete-plus":
    backspaceTriggersAutocomplete: true
    confirmCompletion: "tab"
  blame:
    dateFormat: "DD/MM/YY"
  "bracket-matcher": {}
  build:
    buildOnSave: true
    panelVisibility: "Show on Error"
    stealFocus: false
  "clip-history": {}
  core:
    audioBeep: false
    destroyEmptyPanes: false
    disabledPackages: [
      "welcome"
      "symbols-view"
      "spell-check"
      "build"
      "hyperclick"
      "jsonlint"
      "wordcount"
    ]
    packagesWithKeymapsDisabled: []
    themes: [
      "atom-dark-ui"
      "monokai"
    ]
  "cursor-history": {}
  "easy-motion-redux": {}
  editor:
    fontFamily: "SourceCodePro-Light"
    fontSize: 16
    invisibles: {}
    lineHeight: 1.3
    preferredLineLength: 120
    showInvisibles: true
  "exception-reporting":
    userId: "92fbfa32-55eb-71cf-9895-6d5562807114"
  "git-blame":
    ignoreWhiteSpaceDiffs: true
  "git-diff":
    showIconsInEditorGutter: true
  "git-plus":
