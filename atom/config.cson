    splitPane: "Right"
    wordDiff: false
  jsformat:
    format_on_save: false
  "lazy-motion":
    saveHistoryOnCancel: false
  linter:
    ignoreMatchedFiles: "**/schema.rb"
    lintOnFly: false
    showErrorPanel: false
    showErrorTabFile: false
    statusIconScope: "File"
  "linter-aspell":
    ignoredWords: "config,pearce,fettleton,orc,bonecruncher,aergon,API"
  "linter-rubocop":
    command: "/Users/jack/.rvm/gems/ruby-2.3.0@global/wrappers/rubocop"
  "linter-ruby":
    rubyExecutablePath: "/Users/jack/.rvm/rubies/ruby-2.1.3/bin/ruby"
  "linter-scss-lint":
    executablePath: "/Users/jack/.rvm/wrappers/ruby-2.1.3/scss-lint"
  "recent-finder": {}
  "spell-check": {}
  "split-diff":
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    syncHorizontalScroll: true
  term2: {}
  "tree-view":
    hideIgnoredNames: true
  "vim-mode":
    useClipboardAsDefaultRegister: true
    useSmartcaseForSearch: true
  "vim-mode-next":
    useClipboardAsDefaultRegister: true
    useSmartcaseForSearch: true
  "vim-mode-plus":
