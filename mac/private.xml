  <item>
    <name>My home mods</name>

    <item>
      <name>Jack's vi navigation and other central symbol shortcuts</name>
      <appendix>Use Space to enable symbol layer.</appendix>
      <appendix><![CDATA[  h j k l -> arrow keys ]]></appendix>
      <appendix><![CDATA[  u i -> ( ) ]]></appendix>
      <appendix><![CDATA[  m , -> { } ]]></appendix>
      <appendix><![CDATA[  p -> | ]]></appendix>
      <appendix><![CDATA[  . - > _ ]]></appendix>
      <appendix></appendix>
      <appendix>(You can adjust timeout by "__BlockUntilKeyUp__ Timeout" in Parameters tab.</appendix>
      <appendix></appendix>
      <identifier>remap.my_home_mods</identifier>

      {{ SIMPLE_VI_MODE_CONDITION }}

      <autogen>
        __KeyOverlaidModifier__
        KeyCode::SPACE,

        <!--
          Hi-jack the virtual modifier SIMPLE_VI_MODE_2
